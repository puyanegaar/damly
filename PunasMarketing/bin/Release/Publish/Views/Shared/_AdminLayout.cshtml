    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.24.js"></script>
    @*<script src="~/Scripts/bootstrap.min.js"></script>*@
    <script src="~/Content/AdminHtml/js/vendor.min.js"></script>
    <script src="~/Content/AdminHtml/js/elephant.min.js"></script>
    <script src="~/Content/AdminHtml/js/application.min.js"></script>
